{
  "name": "gaze",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "devDependencies": {
    "@gaze/eslint-config": "*",
    "@gaze/tsconfig": "*",
    "eslint-config-turbo": "0.0.7",
    "prettier": "latest",
    "turbo": "latest",
    "webpack": "^5.75.0"
  },
  "engines": {
    "node": ">=14.0.0"
  }
}
